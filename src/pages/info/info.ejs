    <section  class="info-section__food-offerings">
        <div class="info-section__food-offerings__items">
            <% [{id: "1", name: "food1", desc: "food1"}, {id: "2", name: "food2", desc: "food2"}, {id: "3", name: "food3", desc: "food3"}].forEach(function(cFood){ %>
                <%- include('../shared/thumbnail', {data: cFood}); %>
              <% }); %>
